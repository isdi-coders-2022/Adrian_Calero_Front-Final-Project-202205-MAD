    "name": "your-solution",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@firebase/storage": "^0.9.9",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.9.0",
        "@reduxjs/toolkit": "^1.8.3",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^14.2.1",
        "@types/jest": "^27.5.2",
        "@types/node": "^17.0.45",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "firebase": "^9.9.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-lazy-route": "^0.1.0",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "sweetalert2": "^11.4.22",
        "typescript": "^4.7.4",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "test:prod": "react-scripts test --watchAll --collect-coverage --ci --silent --watchAll=false",
        "test-all": "npm test -- --collect-coverage --watchAll"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/App.tsx",
            "!src/index.tsx",
            "!src/models/*.ts",
            "!src/services/test-utils.tsx",
            "!src/serviceWorker.ts",
            "!src/reportWebVitals.ts",
            "!src/redux/**/action.types.ts",
            "!src/interfaces/*.ts",
            "!src/react-app-env.d.ts"
        ]
    }
}
